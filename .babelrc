{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8.11"
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-react-jsx",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ],
    ["import-rename", { "^(.*)\\.jsx$": "$1" }]
  ]
}
